import pandas as pd
import numpy as np
# 读取CSV文件
df1 = pd.read_csv('all_a_stock_data.csv')
df2 = pd.read_csv('index_data.csv')
df3 = pd.read_csv('us_tycr_data.csv')

# 使用concat函数合并DataFrame
data_concat = pd.concat([df1, df2, df3], ignore_index=True)

data_concat = pd.read_csv(
data_concat.fillna(np.nan, inplace=True)

# 保存合并后的数据到新的CSV文件
data_concat.to_csv('merged_data.csv', index=False)
